<Window x:Class="reletivepathfun.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition></RowDefinition>
            <RowDefinition></RowDefinition>
            <RowDefinition></RowDefinition>
        </Grid.RowDefinitions>
        <Button Grid.Row="0" x:Name="folderBrowserDialogButton">Folder Browser Dialog</Button>
        <Button Grid.Row="1" x:Name="folderBrowserDialogButton2">Folder Browser Dialog</Button>
        <Button x:Name="reletive" Grid.Row="2" Click="Reletive_OnClick"></Button>
    </Grid>
</Window>
